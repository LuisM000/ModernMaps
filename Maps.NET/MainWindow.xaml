                                <Border BorderBrush="#FFC29B29" Margin="-3,-2,0,0" BorderThickness="1" CornerRadius="2">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFFFD075" Offset="1"/>
                                            <GradientStop Color="#FFFFD999"/>
                                            <GradientStop Color="#FFFFC45D" Offset="0.353"/>
                                        </LinearGradientBrush>
                                    </Border.Background>
                                    <StackPanel Margin="5">
                                    <TextBlock x:Name="txtAdvice" TextWrapping="Wrap" Width="260"/>
                                    </StackPanel>
                                </Border>
                            </r:RibbonGalleryCategory>
